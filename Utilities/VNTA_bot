                    }

                }
            });
            s4dmessage.react('üëç');
            s4dmessage.delete();
            s4dmessage.channel.send(String('Message deleted to keep the command confidential.'));
        }
        if (command == 'v.say') {
            s4dmessage.delete();
            s4dmessage.channel.send({
                embed: {
                    title: null,
                    color: (colourRandom()),
                    image: {
                        url: null
                    },

                    description: (arguments2.join(' ')),
                    footer: {
