                    }

                }
            });
            s4dmessage.delete();
            s4dmessage.channel.send(String('Message deleted to keep the command confidential.'));
        }
        if (command == 'v.decline_user_full') {
            s4d.client.channels.cache.get('845682302662344756').send({
                embed: {
