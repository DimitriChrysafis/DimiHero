                    }

                }
            });
        }
        if (command == 'v.aft_user') {
            if (((s4dmessage.channel || {}).name) != '„Éªtesters-results' || ((s4dmessage.channel || {}).name) != '„Éªresults') {
                s4dmessage.channel.send({
                    embed: {
